{
    "ordererAddress": "0.0.0.0",
    "ordererPort": "5005",
    "couchdbAddress": "0.0.0.0",
    "couchdbPort": "5984",
    "vp0Address": "0.0.0.0",
    "vp0Port": "7061",
    "kafkaAddress": "0.0.0.0",
    "kafkaPort": "9092",
    "evtAddress": "0.0.0.0",
    "evtPort": "6051",
    "version": "2",
    "networks":
            "bridge",
    "services": {
        "zookeeper": {
            "image": "hyperledger/fabric-zookeeper",
            "container_name": "zookeeper",
            "networks": [
                 "bridge"
            ]
        },
        "kafka": {
            "image": "hyperledger/fabric-kafka",
            "environment": {
                 "KAFKA_BROKER_ID": "0",
                 "KAFKA_ZOOKEEPER_CONNECT": "zookeeper:2181",
                 "KAFKA_DEFAULT_REPLICATION_FACTOR": "3",
                 "KAFKA_UNCLEAN_LEADER_ELECTION_ENABLE": "false"
            },
            "depends_on": [
                 "zookeeper"
            ],
            "container_name": "kafka0",
            "ports": [
                 "9092:9092"
            ],
            "networks": [
                 "bridge"
            ]
        },
        "orderer": {
            "image": "hyperledger/fabric-orderer",
            "environment": {
                "ORDERER_GENERAL_LOGLEVEL": "debug",
                "ORDERER_GENERAL_LISTENADDRESS": "0.0.0.0",
                "ORDERER_GENERAL_GENESISMETHOD": "file",
                "ORDERER_GENERAL_GENESISFILE": "/var/hyperledger/orderer/orderer.block",
                "ORDERER_GENERAL_LOCALMSPID": "OrdererMSP",
                "ORDERER_GENERAL_LOCALMSPDIR": "/var/hyperledger/orderer/localMspConfig"
            },
            "working_dir": "/opt/gopath/src/github.com/hyperledger/fabric",
            "command": "orderer",
            "volumes": [
            ],
            "ports": [
                 "7050:7050"
            ],
            "container_name": "orderer",
            "depends_on": [
                 "kafka"
            ],
            "networks": [
                 "bridge"
            ]
        },
        "peer": {
            "image": "hyperledger/fabric-peer",
            "environment": {
                 "CORE_PEER_ADDRESSAUTODETECT": "true",
                 "CORE_VM_ENDPOINT": "unix:///host/var/run/docker.sock",
                 "CORE_LOGGING_LEVEL": "DEBUG",
                 "CORE_NEXT": "true",
                 "CORE_PEER_ENDORSER_ENABLED": "true",
                 "CORE_PEER_PROFILE_ENABLED": "true",
                 "CORE_PEER_GOSSIP_IGNORESECURITY": "true",
                 "CORE_PEER_GOSSIP_ORGLEADER": "true",
                 "CORE_PEER_ID": "peer",
                 "CORE_PEER_GOSSIP_BOOTSTRAP": "peer0:5005",
                 "CORE_PEER_COMMITTER_LEDGER_ORDERER": "orderer:5005",
                 "CORE_PEER_MSPCONFIGPATH": "/var/hyperledger/msp/peer0/localMspConfig",
                 "CORE_PEER_LOCALMSPID": "Org0MSP"
            },
            "volumes": [
                  "/var/run/:/host/var/run/"
            ],
            "ports": [
                  "PORTID:7051"
            ],
            "depends_on": [
                  "orderer"
            ],
            "working_dir": "/opt/gopath/src/github.com/hyperledger/fabric/peer",
            "command": "peer node start --peer-defaultchain=false",
            "container_name": "vpID",
            "networks": [
                 "bridge"
            ]
        },
        "cli": {
            "container_name": "cli",
            "image": "hyperledger/fabric-peer",
            "tty": "true",
            "environment": {
                  "GOPATH":"/opt/gopath",
                  "CORE_PEER_ADDRESSAUTODETECT":"true",
                  "CORE_VM_ENDPOINT":"unix:///host/var/run/docker.sock",
                  "CORE_LOGGING_LEVEL":"DEBUG",
                  "CORE_NEXT":"true",
                  "CORE_PEER_ID":"cli",
                  "CORE_PEER_ENDORSER_ENABLED":"true",
                  "CORE_PEER_COMMITTER_LEDGER_ORDERER":"orderer:5005",
                  "CORE_PEER_ADDRESS":"peer0:7051",
                  "CORE_PEER_GOSSIP_IGNORESECURITY":"true",
                  "CORE_PEER_LOCALMSPID":"Org0MSP",
                  "CORE_PEER_MSPCONFIGPATH":"/opt/gopath/src/github.com/hyperledger/fabric/msp/sampleconfig"
            },
            "working_dir": "/opt/gopath/src/github.com/hyperledger/fabric/peer",
            "command": "sh -c './script.sh | tee  results.log 2>&1; sleep 100000'",
            "volumes": [
                    "/var/run/:/host/var/run/"
            ],
            "depends_on": [
                  "orderer0",
                  "peer0"
            ],
            "networks": [
                  "bridge"
            ]
        }
    }
}
